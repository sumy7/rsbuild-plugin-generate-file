  "files": [
    "dist"
  ],
  "dependencies": {
    "@rsbuild/shared": "^0.4.0",
    "ejs": "^3.1.9",
    "js-yaml": "^4.1.0",
    "mime-types": "^2.1.35",
    "picocolors": "^1.0.0"
  }
